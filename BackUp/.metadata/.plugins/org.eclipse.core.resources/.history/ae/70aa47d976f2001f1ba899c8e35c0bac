//		System.out.println(map.containsKey(1)+" "+ map.containsValue("one"));
		
		

		Map<Integer, String> map2 = new HashMap<Integer, String>();
		map2.putAll(map);
		
//		System.out.println(map2);
		
		map.merge(3, "tree", (oldValue,newValue)-> newValue);
		System.out.println(map);

	}
}
